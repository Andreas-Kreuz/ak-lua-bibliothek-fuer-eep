{
  "name": "web-app-react",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "homepage": ".",
  "scripts": {
    "dev": "npm run build --prefix ../web-shared && vite --host",
    "build": "npm run build --prefix ../web-shared && tsc && vite build",
    "preview": "vite preview",
    "start-server-for-playing": "npm run start --prefix ../web-server",
    "start-playing": "concurrently -k \"npm run start-server-for-playing\" \"npm run dev\"",
    "start-server-electron": "npm run start-e2e-electron-react --prefix ../web-server",
    "start-server-headless": "npm run start-e2e-headless-react --prefix ../web-server",
    "cy-tests": "concurrently -k \"npm run start-server-electron\" \"npm run dev\" \"cypress open\"",
    "cy-tests-run-gui": "concurrently -k \"npm run start-server-electron\" \"npm run dev\" \"cypress run --headed",
    "cy-tests-run-headless": "concurrently -k \"npm run start-server-headless\" \"cypress run --config baseUrl=http://localhost:5173\"",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fontsource/anek-latin": "^4.5.4",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-qr-code": "^2.0.11",
    "react-router-dom": "^6.6.1",
    "socket.io-client": "^4.5.4",
    "web-shared": "file:../web-shared",
    "web-vitals": "^3.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "concurrently": "^7.6.0",
    "cypress": "^12.5.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.3"
  }
}
