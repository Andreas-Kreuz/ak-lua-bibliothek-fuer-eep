{
  "name": "@ak/web-server",
  "version": "0.0.1",
  "description": "Lua Server f√ºr EEP - ein Server im Fenster",
  "type": "commonjs",
  "private": true,
  "scripts": {
    "build-shared": "yarn workspace @ak/web-shared run build",
    "build-app": "yarn workspace @ak/web-app run build",
    "copy-web-app": "yarn run pub-del ; yarn run pub-mkdir && yarn run pub-copy",
    "bash": "bash",
    "pub-copy": "yarn run bash -c \"cp -r ../web-app/dist/* build/public_html/\"",
    "pub-del": "yarn run bash -c \"test -d build/public_html && rm -rf build/public_html\"",
    "pub-mkdir": "yarn run bash -c \"mkdir -p build/public_html\"",
    "build": "yarn build-shared && yarn build-app && yarn copy-web-app && tsc --build",
    "watch": "tsc -w",
    "lint": "eslint -c .eslintrc.json --ext .ts src",
    "start": "yarn run build && electron ./build/main/index.js",
    "start-headless": "tsc && node ./build/HeadlessStart.js",
    "start-with-app": "yarn build && electron ./build/main/index.js",
    "start-e2e-headless": "tsc && node ./build/HeadlessStart.js --testmode --exchange-dir ../web-app/cypress/io",
    "start-e2e-electron": "tsc && electron ./build/main/index.js --testmode --exchange-dir ../web-app/cypress/io",
    "package-win": "yarn build && electron-builder build --win --x64",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "persist-eep-state": "tsc && node ./build/server/eep-service/eep-state-reader.js"
  },
  "repository": "https://github.com/ak-lua-bibliothek-fuer-eep",
  "keywords": [
    "EEP",
    "Lua"
  ],
  "main": "./build/main/index.js",
  "build": {
    "electronVersion": "22.3.0",
    "appId": "io.github.andreas-kreuz.ak-lua-library-for-eep",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
      "!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
      "!**/node_modules/*.d.ts",
      "!**/node_modules/.bin",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!.editorconfig",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
      "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
      "!**/{appveyor.yml,.travis.yml,circle.yml}",
      "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}"
    ],
    "directories": {
      "buildResources": "resources",
      "output": "dist"
    },
    "mac": {
      "category": "Application"
    },
    "win": {
      "target": [
        "portable"
      ],
      "compression": "maximum",
      "artifactName": "lua-server-for-eep.${ext}"
    }
  },
  "author": "Andreas-Kreuz",
  "license": "MIT",
  "devDependencies": {
    "@types/command-line-args": "^5.2.0",
    "@types/command-line-usage": "^5.0.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/node": "^13.13.4",
    "@types/tail": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "del-cli": "^4.0.1",
    "electron": "^23.1.1",
    "electron-builder": "^23.6.0",
    "eslint": "^8.1.0",
    "npm-run-all": "^4.1.5",
    "run-script-os": "^1.1.6",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@ak/web-shared": "workspace:^",
    "@electron/typescript-definitions": "^8.10.0",
    "command-line-args": "^5.1.1",
    "command-line-usage": "^6.1.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "rxjs": "^7.5.1",
    "socket.io": "^4.4.0",
    "tail": "^2.2.4"
  }
}
